{
  "name": "DiaryApp",
  "version": "0.9.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "browserify --debug js/index.js > public/bundle.js",
    "build-dist": "NODE_ENV=production browserify js/index.js | uglifyjs -m > public/bundle.min.js",
    "watch": "watchify -v --debug js/index.js -o public/bundle.js",
	"start": "node app.js"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2010jr/DiaryApp.git"
  },
  "author": "Jun Kusahana",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/2010jr/DiaryApp/issues"
  },
  "dependencies": {
	 "uglify-js": "~2.4.24",
	 "browserify": "~11.0.1",
	 "react": "~0.13.3",
	 "reactify": "~1.1.1",
	 "watchify": "~3.4.0",
	 "express" : "~4.13.3",
	 "mongodb" : "~2.1.0",
	 "ldapjs" : "~1.0.0",
	 "body-parser" : "~1.14.1",
	 "assert" : "~1.3.0",
	 "cfenv" : "~1.0.3"
  }
}
