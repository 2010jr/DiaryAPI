{
  "name": "DiaryApp",
  "version": "0.9.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "build": "browserify --debug js/index.js > public/bundle.js",
    "build-dist": "NODE_ENV=production browserify js/index.js | uglifyjs -m > public/bundle.min.js",
    "watch": "watchify -v --debug js/index.js -o public/bundle.js",
    "start": "node app.js",
    "test": "mocha"
  },
  "browserify": {
    "transform": [
      [
        "reactify",
        {
          "harmony": true
        }
      ]
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/2010jr/DiaryApp.git"
  },
  "author": "Jun Kusahana",
  "license": "BSD",
  "bugs": {
    "url": "https://github.com/2010jr/DiaryApp/issues"
  },
  "dependencies": {
    "uglify-js": "~2.4.24",
    "browserify": "~11.0.1",
    "react": "~0.14.7",
    "react-dom": "~0.14.7",
    "reactify": "~1.1.1",
    "watchify": "~3.4.0",
    "express": "~4.13.3",
    "mongodb": "~2.1.0",
    "ldapjs": "~1.0.0",
    "body-parser": "~1.14.1",
    "assert": "~1.3.0",
    "cfenv": "~1.0.3",
    "jasmine": "~2.4.1",
    "d3": "~3.5.15",
    "react-addons-transition-group": "~0.14.7",
    "react-addons-css-transition-group": "~0.14.7",
    "jquery": "~3.1.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-config-eslint": "^3.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "mocha": "^3.2.0"
  }
}
